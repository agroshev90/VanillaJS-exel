{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi":"off",
    "require-jsdoc":"off",
    "eol-last":"off",
    "comma-dangle":"off",
    "no-trailing-spaces":"off",
    "arrow-parens":"off"

  },
  "extends": [
    "eslint:recommended", "google"
  ]
}